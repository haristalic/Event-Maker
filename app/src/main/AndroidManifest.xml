<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.something.better">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:allowBackup="true"
        android:icon="@drawable/adix"
        android:label="@string/app_name"
        android:roundIcon="@drawable/adix"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
    <activity android:name=".MainActivity">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
        <activity android:name=".FeedActivity"
            android:label="Upcoming AdixEvents List"/>
        <activity android:name=".RegisterActivity" />
        <activity android:name=".NewSocialActivity"
            android:label="Create AdixEvent">

        </activity>
        <activity android:name=".DetailActivity" android:label="AdixEvent Details"/>
        <activity android:name=".VerifyEmailActivity"
            android:label="Verify Email"/>
        <activity android:name=".UsersInterestedActivity"></activity>

    </application>
</manifest>
